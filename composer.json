{
    "name": "zapheus/psr-11-bridge",
    "description": "PSR-11 bridge for Zapheus framework.",
    "keywords": ["bridge", "container", "psr-11", "zapheus"],
    "homepage": "https://github.com/zapheus/psr-11-bridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/container": "~1.0",
        "zapheus/zapheus": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Zapheus\\Bridge\\Psr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zapheus\\Bridge\\Psr\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}